
public class Ejercicio5_INCREMENTO {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
			int N=5;
				System.out.println("El valor actual de N es:" +N);
			for (int i= 0; i <77; i++);
			
			int Suma;
			Suma=N+i;
			System.out.println("El valor actual de N es:" +Suma);
			
	}

}
