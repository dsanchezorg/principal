import java.util.Scanner;
public class Ejercicio5_ComparaString {

	public static void main(String[] args) {
		Scanner teclado=new Scanner(System.in);
		
		String compara1="", compara2="";
		int nCadenas=0, i=0, largo=0, largo2=0, aux=0;
		
		nCadenas=(int)(Math.random()*10)+1;
		
		System.out.println("Compararemos "+nCadenas+" frases");
		for(i=1;i<nCadenas;i++){
			aux=i+1;
			System.out.println("Introduce la "+i+"ยบ frase");
			compara1=teclado.nextLine();
				largo=compara1.length();
			System.out.println("Introduce la "+i+aux+"ยบ frase");
			compara1=teclado.nextLine();
				largo2=compara2.length();
		}
		
	}

}
